{
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/pages/404.html",
            "status": 404
        }
    ],
    "crons": [
        {
            "path": "/api/cron",
            "schedule": "42 11 * * *"
        }
    ]
}